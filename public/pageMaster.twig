<!DOCTYPE html>
<html lang="es">
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <meta charset="utf-8" />
        <title>Caliper - CR EQUIPOS</title>
        <link rel="shortcut icon" href="{{ 'template/images/crIcon.png'|baseUrl}}"/>
        <meta name="keywords" content="crequipos, calibraciones, reportes, caliper, certificados, sintech, equipos medicos"/>
        <meta name="author" content="zeraling">
        <meta name="description" content="Caliper - Sistema de calibraciones de CR EQUIPOS SA. "/>
        <!-- control de contenido -->
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
        <meta http-equiv="cache-control" content="no-cache" /> 
        <meta http-equiv="Pragma" content="no-cache" />
        <meta http-equiv="Expires" content="0" />
        <!-- bootstrap & fontawesome -->
        {% block style_base %}
            <link rel="stylesheet" href="{{ 'template/css/bootstrap.min.css'|baseUrl}}" />
            <link rel="stylesheet" href="{{ 'plugins/font-awesome/css/font-awesome.min.css'|baseUrl}}"/>
            <!-- jquery-ui -->
            <link rel="stylesheet" href="{{ 'plugins/jquery-ui/jquery-ui.min.css'|baseUrl}}" />
            <!-- Noty -->
            <link rel="stylesheet" href="{{ 'template/css/jquery.gritter.min.css'|baseUrl}}" />
            <!-- JS Base -->
            <script type="text/javascript" src="{{ 'template/jquery/jquery-3.2.1.min.js'|baseUrl}}"></script>
            <script type="text/javascript" src="{{ 'template/gears.js'|baseUrl}}"></script>
            <script type="text/javascript" src="{{ 'template/load.js'|baseUrl}}"></script>
            <!-- text fonts -->
            <link rel="stylesheet" href="{{ 'template/css/fonts.googleapis.com.css'|baseUrl}}" />
        {% endblock %}
    {% block style_plugin %}{% endblock %}
    {% block style_template %}
        <!-- ace styles -->
        <link rel="stylesheet" href="{{ 'template/css/ace.css'|baseUrl}}" class="ace-main-stylesheet" id="main-ace-style" />
        <!--[if lte IE 9]>
            <link rel="stylesheet" href="{{ 'template/css/ace-part2.min.css'|baseUrl}}" class="ace-main-stylesheet" />
        <![endif]-->
        <link rel="stylesheet" href="{{ 'template/css/ace-skins.min.css'|baseUrl}}" />
        <link rel="stylesheet" href="{{ 'template/css/ace-rtl.min.css'|baseUrl}}" />
        <!--[if lte IE 9]>
            <link rel="stylesheet" href="{{ 'template/css/ace-ie.min.css'|baseUrl}}" />
        <![endif]-->
        <!-- ace settings handler -->
        <script type="text/javascript" src="{{ 'template/jquery/ace-extra.min.js'|baseUrl}}"></script>
        <!-- HTML5shiv and Respond.js for IE8 to support HTML5 elements and media queries -->
        <!--[if lt IE 9]>
            <script type="text/javascript" src="{{ 'template/jquery/html5shiv.min.js'|baseUrl}}"></script>
            <script type="text/javascript" src="{{ 'template/jquery/respond.min.js'|baseUrl}}"></script>
        <![endif]-->
    {% endblock %}
</head>
{% block body %}
    <body class="no-skin">

        <div id="navbar" class="navbar navbar-default navbar-collapse h-navbar ace-save-state nav-header-bg-cr">
            <div class="navbar-container" id="navbar-container">
                <button type="button" class="navbar-toggle menu-toggler pull-left" id="menu-toggler" data-target="#sidebar">
                    <span class="sr-only">Menu</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div class="navbar-header pull-left">
                    <a id="baseRoot" href="{{ ''|baseUrl }}" root="{{ ''|baseUrl }}" class="navbar-brand"><small><span>°</span><b> Caliper </b><span>°</span></small> </a>
                </div>
                <div class="navbar-buttons navbar-header pull-right" role="navigation">
                    <ul class="nav ace-nav">
                        
                        <li class="grey dropdown-modal">
                            <a data-toggle="dropdown" href="#" class="dropdown-toggle">
                                <img class="nav-user-photo" src="{{'template/images/avatars/user.jpg'|baseUrl}}" alt="{{dtaUser.nombres}}'" />

                                <span class="user-info">
                                    <small id="dtaEmpleado" code="{{userCode}}">{{ dtaUser.nombres}},</small>{{dtaUser.cargo}}
                                </span>
                                <i class="ace-icon fa fa-caret-down"></i>
                            </a>
                            <ul class="user-menu dropdown-menu-right dropdown-menu dropdown-yellow dropdown-caret dropdown-close">
                                <li><a href="{{'usuario'|baseUrl}}"><i class="ace-icon fa fa-user"></i> Perfil</a></li>
                                <li><a href="#"><i class="ace-icon fa fa-cog"></i> Ayuda</a></li>
                                <li class="divider"></li>
                                <li><a  href="{{loginKey|baseUrl}}" ><i class="ace-icon fa fa-power-off"></i> Salir</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div><!-- /.navbar-container -->
        </div>
        <div class="main-container ace-save-state" id="main-container">
            <div id="sidebar" class="sidebar h-sidebar navbar-collapse collapse">
                {{ include('pageMenu.twig') }}
            </div>

            <div class="main-content">
                <div class="main-content-inner">
                {%block content %}{%endblock%}
            </div>
        </div><!-- /.main-content -->
        <div class="footer">
            <div class="footer-inner">
                <div class="footer-content">
                    <span class="bigger-120">
                        <span class="red bolder">Caliper</span><b> &copy; {{ 'now'|date('Y') }}  CR EQUIPOS.</b>
                    </span>
                </div>
            </div>
        </div>
        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
            <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
        </a>
    </div><!-- /.main-container -->

    {% block js_base %}
        <!-- Bootstrap  -->
        <script type="text/javascript" src="{{ 'template/jquery/bootstrap.min.js'|baseUrl}}"></script>
        <!-- jQuery UI -->
        <script type="text/javascript" src="{{ 'plugins/jquery-ui/jquery-ui.min.js'|baseUrl}}"></script>
        <script type="text/javascript" src="{{ 'template/jquery/jquery.ui.touch-punch.min.js'|baseUrl}}"></script>
        <!-- specific plugin scripts --> 
        <script type="text/javascript" src="{{ 'template/jquery/jquery.gritter.min.js'|baseUrl}}"></script>
        <!-- ace scripts -->
        <script type="text/javascript" src="{{ 'template/jquery/ace-elements.min.js'|baseUrl}}"></script>
        <script type="text/javascript" src="{{ 'template/jquery/ace.min.js'|baseUrl}}"></script>

        <script type="text/javascript" src="{{ 'application/scripts/siteMaster.js'|baseUrl}}"></script>
    {% endblock %}
{% block js_custom %}{% endblock %}
</body>
{% endblock %}

</html>
